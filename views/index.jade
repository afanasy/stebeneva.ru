extends layout
block content
  - var i = 0;
  script(src='/js/index.js')
  #slides
    each slides, section in conf
      each show, slide in slides
        div(id='slide' + i)
          a(href="/photo/#{section}" title="#{ucfirst(section)}")
            img(src="/photos/#{section}/slides/#{slide}")
            - i++;
